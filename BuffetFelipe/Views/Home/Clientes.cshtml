@using BuffetFelipe.ViewModels.Home
@model BuffetFelipe.ViewModels.Home.ClientesViewModel

@{
    ViewData["Title"] = "Clientes";
}
<h1>@ViewData["Title"]</h1>

<p>Listagem de Clientes</p>
<ul>
    @foreach (Cliente cliente in Model.Clientes)
    {
        <li>@cliente.Nome nascido em @cliente.DataDeNascimento</li>
    }
    
</ul>